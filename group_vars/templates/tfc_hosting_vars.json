{
  "tfc_tf_vars": [
    {
      "key": "environment",
      "value": "${ENVIRONMENT}",
      "description": "",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "primary_domain_name",
      "value": "${DOMAIN_NAME}",
      "hcl": "false"
    },
    {
      "key": "cloudflare_email",
      "value": "${CLOUDFLARE_EMAIL}",
      "description": "",
      "hcl": "false",
      "sensitive": "true"
    },
    {
      "key": "cloudflare_api_key",
      "value": "${CLOUDFLARE_API_KEY}",
      "description": "",
      "hcl": "false",
      "sensitive": "true"
    },
    {
      "key": "key_pair_name",
      "value": "${OPS_USER}_ec2_${ENVIRONMENT}",
      "description": "The AWS SSL key pair for accessing EC2 instances",
      "hcl": "false"
    }
  ]
  }
