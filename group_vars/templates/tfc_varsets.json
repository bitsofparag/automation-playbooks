{
  "tfc_varsets": [
    {
      "varset_name": "CloudVars",
      "varset_description": "All cloud vars shared across workspaces",
      "is_global": true,
      "vars": [
        {
          "key": "AWS_ACCESS_KEY_ID",
          "value": "${AWS_ACCESS_KEY_ID}",
          "description": "",
          "hcl": "false",
          "sensitive": "false",
          "category": "env"
        },
        {
          "key": "AWS_SECRET_ACCESS_KEY",
          "value": "${AWS_SECRET_ACCESS_KEY}",
          "description": "",
          "hcl": "false",
          "sensitive": "true",
          "category": "env"
        },
        {
          "key": "aws_region",
          "value": "${AWS_REGION}",
          "description": "",
          "hcl": "false",
          "sensitive": "false",
          "category": "terraform"
        },
        {
          "key": "gcp_region",
          "value": "${GCP_REGION}",
          "description": "",
          "hcl": "false",
          "sensitive": "false",
          "category": "terraform"
        },
        {
          "key": "GOOGLE_CREDENTIALS",
          "value": "${GCP_CREDENTIALS}",
          "description": "",
          "hcl": "false",
          "sensitive": "true",
          "category": "env"
        },
        {
          "key": "HCLOUD_TOKEN",
          "value": "${HCLOUD_TOKEN}",
          "description": "",
          "hcl": "false",
          "sensitive": "true",
          "category": "env"
        }
      ]
    }
  ]
}
