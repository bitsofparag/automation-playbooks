{
  "tfc_tf_vars": [
    {
      "key": "environment",
      "value": "${ENVIRONMENT}",
      "description": "",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "backend_services",
      "value": "[]",
      "description": "",
      "hcl": "true",
      "sensitive": "false"
    },
    {
      "key": "app_domain_name",
      "value": "${DOMAIN_NAME}",
      "description": "The main domain name under which the backend apps are hosted",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "key_pair_name",
      "value": "${OPS_USER}_vm_${ENVIRONMENT}",
      "description": "The AWS SSL key pair for accessing EC2 instances",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "custom_ssh_port",
      "value": "${CUSTOM_SSH_PORT}",
      "description": "Custom SSH port for accessing VMs. If not set, the default SSH port (22) will be used",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "health_check_path",
      "value": "/ping/",
      "description": "Health check path for the default target group",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "default_log_retention_in_days",
      "value": "14",
      "description": "",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "userdata_ssh_pubkey",
      "value": "${SSH_PUBKEY}",
      "description": "",
      "hcl": "false",
      "sensitive": "true"
    },
    {
      "key": "auth_sentry_dsn",
      "value": "${AUTH_SENTRY_DSN}",
      "description": "",
      "hcl": "false",
      "sensitive": "false"
    },
    {
      "key": "apps_sentry_dsn",
      "value": "${APPS_SENTRY_DSN}",
      "description": "",
      "hcl": "false",
      "sensitive": "false"
    }
  ]
}
