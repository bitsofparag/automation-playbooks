    {
      "key": "add_bastion",
      "value": "false",
      "hcl": "true"
    },
    {
      "key": "enable_nat",
      "value": "false",
      "hcl": "true"
    },
    {
      "key": "primary_domain_name",
      "value": "${DOMAIN_NAME}"
    }
  ]
}
